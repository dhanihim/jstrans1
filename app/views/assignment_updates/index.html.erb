<p id="notice"><%= notice %></p>

<h1>Assignment Updates</h1>

<table>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Document Type</th>
      <th>Document Path</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assignment_updates.each do |assignment_update| %>
      <tr>
        <td><%= assignment_update.uid %></td>
        <td><%= assignment_update.document_type %></td>
        <td><%= assignment_update.document_path %></td>
        <td><%= link_to 'Show', assignment_update %></td>
        <td><%= link_to 'Edit', edit_assignment_update_path(assignment_update) %></td>
        <td><%= link_to 'Destroy', assignment_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assignment Update', new_assignment_update_path %>
