<h4>Customer Location Pricelists</h4>

<br>

<%= link_to 'New Customer Location Pricelist', new_customer_location_pricelist_path, class:"btn btn-primary" %>

<br>
<br>

<div class="table-responsive">
<table class="table">
  <thead>
    <tr>
      <th>per20ft</th>
      <th>per40ft</th>
      <th>per20od</th>
      <th>per21ft</th>
      <th>per20fr</th>
      <th>per40fr</th>
      <th>Ppncategory</th>
      <th>Active</th>
      <th>Description</th>
      <th>Customer location from</th>
      <th>Customer location to</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customer_location_pricelists.each do |customer_location_pricelist| %>
      <tr>
        <td><%= customer_location_pricelist.per20ft %></td>
        <td><%= customer_location_pricelist.per40ft %></td>
        <td><%= customer_location_pricelist.per20od %></td>
        <td><%= customer_location_pricelist.per21ft %></td>
        <td><%= customer_location_pricelist.per20fr %></td>
        <td><%= customer_location_pricelist.per40fr %></td>
        <td><%= customer_location_pricelist.ppncategory %></td>
        <td><%= customer_location_pricelist.active %></td>
        <td><%= customer_location_pricelist.description %></td>
        <td><%= customer_location_pricelist.customer_location.address %></td>
        <td><%= customer_location_pricelist.location.name %></td>
        <td><%= link_to 'Show', customer_location_pricelist %></td>
        <td><%= link_to 'Edit', edit_customer_location_pricelist_path(customer_location_pricelist) %></td>
        <td><%= link_to 'Destroy', customer_location_pricelist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

