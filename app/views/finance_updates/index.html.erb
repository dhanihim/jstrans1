<p id="notice"><%= notice %></p>

<h1>Finance Updates</h1>

<table>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Document path</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @finance_updates.each do |finance_update| %>
      <tr>
        <td><%= finance_update.uid %></td>
        <td><%= finance_update.document_path %></td>
        <td><%= finance_update.description %></td>
        <td><%= link_to 'Show', finance_update %></td>
        <td><%= link_to 'Edit', edit_finance_update_path(finance_update) %></td>
        <td><%= link_to 'Destroy', finance_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Finance Update', new_finance_update_path %>
